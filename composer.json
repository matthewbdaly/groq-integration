{
    "name": "matthewbdaly/groq-integration",
    "description": "An API integration for Groq Cloud",
    "type": "library",
    "require": {
        "saloonphp/saloon": "v3.x-dev"
    },
    "require-dev": {
        "pestphp/pest": "3.x-dev",
        "vimeo/psalm": "6.x-dev",
        "slevomat/coding-standard": "8.x-dev",
        "infection/infection": "dev-master"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Matthewbdaly\\GroqIntegration\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Matthew Daly",
            "email": "450801+matthewbdaly@users.noreply.github.com"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
